#include<iostream> 
using namespace std;
int main()
{	
int a, b, c, d, e, f, g;
cout<<"The value of 'a' is:"<<endl;
cin>>a; 
cout<<"The value of 'b' is:"<<endl;
cin>>b;
 cout << "The value of c is:"<<endl;
cin>>c;
cout<<"The value of 'd' is:"<<endl;
cin>>d;
cout<<"The value of 'e' is:"<<endl;
cin>>e;
cout<<"The value of 'f' is:"<<endl;
cin>>f;
cout<<"The value of 'g' is:"<<endl;
cin>>g;
cout<< "The solution of the given expression is: "<< ( ( a + b / c* d – e ) * ( f – g ) ) ;
return 0;   
}
