#include <iostream>
using namespace std;
int main(){
char ch;
cout<< “Enter any character: ”;
cin>>ch;
if (ch > = 65 && ch <=90)
cout<< “You entered a Capital letter. ” ;
else  if (ch > = 97 && ch <=122)
cout<< “You entered a Small case letter. ” ;
else  if (ch > = 48 && ch <=57)
cout<< “You entered a Digit. ” ;
else
cout<< “You entered a Special character. ” ;
return 0;
} 
