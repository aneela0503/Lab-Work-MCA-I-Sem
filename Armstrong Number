#include < iostream >
using namespace std;
int main(){
int num, r, sum = 0, ArmNum ;
cout << “ Enter number to check: ”
cin >> num ;
ArmNum = num ;
while ( num > r ){
r = num % 10 ;
sum = sum + ( r * r * r );
num = num / 10;
}
if ( ArmNum = = 0) {
cout << “ Entered number is Armstrong number. ” << endl ;
else 
cout << “Entered number is not Armstrong number. ” << endl ;
return 0 ;  
}
